                                        },
                                        "email_verified_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2020-01-27T17:50:45Z"
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2020-01-27T17:50:45Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2020-01-27T17:50:45Z"
                                        },
                                        "first_name": {
                                            "type": "string",
                                            "example": "John"
                                        },
                                        "last_name": {
                                            "type": "string",
                                            "example": "Doe"
                                        },
                                        "second_name": {
                                            "type": "string",
                                            "example": "Smith"
                                        },
                                        "birthday": {
                                            "type": "string",
                                            "format": "date",
                                            "example": "1995-05-16"
                                        },
                                        "sex": {
                                            "type": "string",
                                            "example": "male"
                                        }
                                    },
                                    "type": "object"
                                }
